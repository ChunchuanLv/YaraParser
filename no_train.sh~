java -jar  jar/YaraParser.jar train -train-file ../data/train.dep -dev ../data/dev.dep  -model ../data/model/no_model iter:10 -punc punc_files/wsj.puncs
java -jar jar/YaraParser.jar parse_partial -input ../data/test.dep  -out ../data/0model.dep  -model ../data/model/no_model_best  -punc punc_files/wsj.puncs
java -jar jar/YaraParser.jar eval -gold ../data/test.dep -parse ../data/0model.dep -punc punc_files/wsj.puncs

